{
  "env": {
    "node": true,
    "es6": true
  },
  "extends": ["prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-useless-constructor": 0,
    "space-before-function-paren": "off",
    "import/first": "off",
    "no-unused-vars": "off"
  }
}
